<odoo>
    <data>
        <record id="sale_order_inherit_form_view" model="ir.ui.view">
            <field name="name">sale_order_inherit_form_view</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet/notebook/page[@name='order_lines']/field[@name='order_line']/tree/field[@name='price_subtotal']" position="after">
                    <!-- Add your fields or attributes here -->
                  <field name="customer" invisible="1"/>


                </xpath>

                <xpath expr="//sheet/notebook/page[@name='order_lines']/field[@name='order_line']/tree/field[@name='price_unit']" position="replace">
                    <!-- Add your fields or attributes here -->
                  <field name="price_unit" readonly="1"/>


                </xpath>
                <xpath expr="//sheet/notebook/page[@name='other_information']" position="after">
                        <page string="Cars" name="cars">
                            <group>
                                    <field name="cars" />
                            </group>
                        </page>



                </xpath>
            </field>
        </record>
    </data>
</odoo>